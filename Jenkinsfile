pipeline {
  agent any
  stages {
    stage('checkout') {
      steps {
        git(url: 'git@github.com:Myselfvamsireddy/staticwebsite.git', branch: 'raji')
      }
    }

    stage('Buid') {
      steps {
        sh 'ls -ltr'
      }
    }

    stage('upload') {
      steps {
        sh 'ls'
      }
    }

    stage('deploy') {
      steps {
        sh 'ls'
      }
    }

  }
}